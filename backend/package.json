{
  "name": "adonis-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production --ignore-ts-errors",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "debug": "node --inspect=9229 -r ts-node/register ./ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "migration:run": "node ace migration:run",
    "migration:rollback": "node ace migration:rollback --batch=0",
    "migration:status": "node ace migration:status",
    "db:seed": "node ace db:seed -i",
    "test": "node -r @adonisjs/assembler/build/register japaFile.ts",
    "coverage": "nyc yarn test"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.0.0",
    "@types/jsdom": "^16.2.13",
    "@types/mjml": "^4.7.0",
    "@types/pdfmake": "^0.1.20",
    "@types/proxy-addr": "^2.0.0",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.11",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.3",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "5.1.1",
    "get-port": "5.1.1",
    "japa": "^4.0.0",
    "jsdom": "^17.0.0",
    "mailhog": "^4.16.0",
    "nyc": "^15.1.0",
    "pino-pretty": "^5.1.3",
    "prettier": "^2.3.2",
    "sinon": "^13.0.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.9.2",
    "typescript": "~4.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.7",
    "@adonisjs/core": "^5.1.0",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/mail": "^7.2.3",
    "@adonisjs/redis": "^7.0.8",
    "@adonisjs/repl": "^3.0.0",
    "@adonisjs/session": "^6.1.2",
    "@adonisjs/view": "^6.0.8",
    "@types/source-map-support": "^0.5.4",
    "glob-parent": "^6.0.2",
    "https": "^1.0.0",
    "luxon": "^2.0.2",
    "mjml": "^4.10.2",
    "mssql": "^7.2.0",
    "pdfmake": "^0.2.4",
    "phc-bcrypt": "^1.0.7",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
